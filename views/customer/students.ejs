<%- include('../include/header_customer') %>
    <link rel="stylesheet" href="/css/index.css">
    </head>
    <body>
        <div class="row">
            <% Students.forEach(function(element) { %>
                <div class="col-3">
                    <a href="<%= element.facebook %>" target="_blank" >
                  
                    <div class="card" style="width: 20rem">
                        <div class="img__fr">
                            <div class="img__fr-item">
                                <img class="avatar" src="<%= element.image %>" alt="" srcset="">
                            </div>
                        </div>
                       
                        <div class="card-body">
                            <h6>Tên: <%= element.name %>
                            </h6>
                            <p class="card-text">Lớp: <%= element.class %>
                            </p>
                            <p class="card-text">MSSV: <%= element.MSSV %>
                            </p>
                        </div>
                    </div>
                    </a>
                </div>
                <% }); %>
        </div>
 <%- include('../include/script_customer') %>